#!/bin/bash
    clear
    figlet "CHECKUSER" | lolcat
    echo -e ""
    echo -e "\033[1;37m 01 ‚û°Ô∏è\033[1;36m CHECKUSER CONECTA 4G  ü•á\033[0m"
	echo -e "\033[1;37m 02 ‚û°Ô∏è\033[1;36m CHECKUSER D TUNEL ü•â\033[0m"
	echo -e "\033[1;37m 03 ‚û°Ô∏è\033[1;36m CHECKUSER GL TUNNEL ü•à\033[0m"
	echo -e "\033[1;37m 04 ‚û°Ô∏è\033[1;36m CHECKUSER MIRACLE üèÖ\033[0m"
    echo -e "\033[1;37m 00 ‚û°Ô∏è\033[1;31m SAIR \033[0m"
    echo -e "\033[01;31m\033[0m"
			tput civis
			echo -ne "\033[1;31m‚û§ \033[1;32mESCOLHA OP√á√ÉO DESEJADA\033[1;33m\033[1;31m\033[1;37m : ";
			read x
			tput cnorm
			clear
			case $x in
			1 | 01)
			initcheck
			;;
			2 | 02)
            bash <(curl -sL https://raw.githubusercontent.com/DTunnel0/DTCheckUser/master/install.sh)
            ;;
            3 | 03)
            gltunnel
            ;;
            4 | 04)
            gltunnel
            ;;
			0 | 00)
			clear
			menu
			;;
			*)
			echo -e "\033[1;31mOpcao invalida !\033[0m"
			esac
			sleep 2
